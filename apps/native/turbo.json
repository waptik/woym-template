{
	"$schema": "https://turbo.build/schema.json",
	"extends": [
		"//"
	],
	"tasks": {
		"build": {
			"dependsOn": [
				"@woym/api#build"
			],
			"outputs": [
				"dist/**",
				".expo/**",
				".cache/**",
				".expo-shared/**",
				"ios/**",
				"android/**"
			],
			"env": [
				".env.production.local",
				".env.production",
				".env.local",
				".env"
			]
		},
		"dev": {
			"outputs": [
				"dist/**",
				".expo/**",
				".cache/**",
				".expo-shared/**",
				"ios/**",
				"android/**"
			],
			"dependsOn": [
				"@woym/api#build"
			],
			"cache": false,
			"persistent": true,
			"interactive": true,
			"env": [
				".env.production.local",
				".env.production",
				".env.local",
				".env"
			]
		}
	}
}