{
  "name": "@woym/server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "wrangler deploy --dry-run",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "vite --host 0.0.0.0 --port=3001",
    "start": "wrangler dev",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.3",
    "@woym/api": "workspace:*",
    "@woym/auth": "workspace:*",
    "@woym/db": "workspace:*",
    "ai": "^4.3.16",
    "dotenv": "^16.4.7",
    "hono": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.6",
    "@woym/tailwind-config": "workspace:*",
    "@woym/tsconfig": "workspace:*",
    "@woym/schemas": "workspace:*",
    "@woym/workers-types": "workspace:*",
    "tsdown": "^0.12.9",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "catalog:"
  }
}