    "ai": "^4.3.16",
    "dotenv": "^16.4.7",
    "hono": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@woym/tailwind-config": "workspace:*",
    "@woym/tsconfig": "workspace:*",
    "typescript": "catalog:",
    "wrangler": "catalog:"
  }
}