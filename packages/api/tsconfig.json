{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@woym/tsconfig/internal-package.json",
	"compilerOptions": {
		"noEmit": false,
		"baseUrl": ".",
		"paths": {
			"#/*": [
				"./src/*"
			]
		},
		"types": [
			"node",
			"@cloudflare/workers-types"
		]
	},
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
