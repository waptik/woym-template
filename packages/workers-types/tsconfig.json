{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@woym/tsconfig/internal-package.json",
	"compilerOptions": {
		"composite": true,
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"noEmit": false,
		"baseUrl": ".",
		"types": [
			"./worker-configuration.d.ts",
		],
	},
	"include": [
		"worker-configuration.d.ts",
		"src/**/*.ts"
	],
}