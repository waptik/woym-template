{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"display": "Vite React",
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"sourceMap": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	},
	"exclude": ["node_modules"]
}
