{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"$TURBO_DEFAULT$",
				".env*"
			],
			"outputs": [
				"dist/**",
				"ios/**",
				"android/**",
				".expo/**",
				".cache/**",
				".wrangler/**"
			]
		},
		"//#knip": {},
		"//#lint": {},
		"//#lint:fix": {
			"cache": false
		},
		"typecheck": {
			"dependsOn": [
				"^typecheck"
			],
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"@woym/api#dev": {
			"cache": false,
			"persistent": false
		},
		"@woym/auth#dev": {
			"cache": false,
			"persistent": false
		},
		"@woym/db#dev": {
			"cache": false,
			"persistent": false
		},
		"@woym/schemas#dev": {
			"cache": false,
			"persistent": false
		},
		"@woym/workers-types#dev": {
			"cache": false,
			"persistent": false
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"db:push": {
			"cache": false,
			"persistent": true
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"db:migrate": {
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false,
			"persistent": true
		}
	}
}